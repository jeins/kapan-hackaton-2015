{
    "name": "KapanBackend",
    "description": "Kawal Pembangunan By Kumpir",
    "keywords": ["kapan", "kawan", "pembangunan"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "firebase/php-jwt": "~2.0",
        "guzzlehttp/guzzle": "6.5.7",
        "vlucas/phpdotenv": "~1.0",
        "vluzrmos/lumen-cors": "2.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
